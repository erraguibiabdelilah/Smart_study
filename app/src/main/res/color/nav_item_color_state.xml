<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Couleur de l'élément sélectionné (ex: Bleu foncé) -->
    <item android:color="#263A85" android:state_checked="true" />
    
